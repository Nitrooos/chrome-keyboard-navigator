{
  "name": "Keyboard Navigator",
  "version": "1.0",
  "description": "Chrome's extension to navigate pages only with keyboard",
  "permissions": [],
  "background": {
    "service_worker": "background-bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["build/extension-bundle.js"]
    }
  ],
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "manifest_version": 3
}
